#include<stdio.h>
int main()
{
Printf("pick an item from our menu:\n1.apple juice\n2.lime juice\n3.orange juice\n4.pineapple juice\n5.mango juice");
int chioce =0;
Scanf("%d",&choice)
Switch(choice)
{
Case 1:
Printf("you ordered apple juice\n rs:35");
